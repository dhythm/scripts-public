{
  "name": "node-ts-project",
  "version": "1.0.0",
  "description": "Node.js TypeScript execution environment",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "primary-harvest": "tsx src/primary-source-harvester.ts",
    "tts": "tsx src/text-to-speech/cli.ts",
    "stt:batch": "tsx src/speech-to-text/chirp-batch-diarization.ts",
    "source-origin": "tsx src/source-origin-analyzer.ts"
  },
  "keywords": [
    "typescript",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.10"
  },
  "dependencies": {
    "@google-cloud/speech": "^7.2.1",
    "@google-cloud/storage": "^7.18.0",
    "node-html-parser": "^7.0.1",
    "openai": "^6.9.1"
  }
}
